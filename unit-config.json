{
    "listeners": {
      "*:8080": {
        "pass": "routes"
      }
    },
    "routes": [
      {
        "action": {
          "share": [
            "/www$uri",
            "/www/$uri"
          ],
          "fallback": {
            "share": "/www/index.html"
          }
        }
      }
    ]
  }
